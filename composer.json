{
	"name": "helhum/realurl",
	"type": "typo3-cms-extension",
	"description": "RealURL: speaking paths for TYPO3",
	"homepage": "http://typo3.org",
	"license": "GPL-2.0-or-later",

	"require": {
		"typo3/cms-core": "^6.2.6 || ^7.6 || >=8.0.0 <8.8",
		"symfony/polyfill-mbstring": "^1.7"
	},
	"replace": {
		"typo3-ter/realurl": "self.version"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Tx\\Realurl\\": "Classes"
		}
	}
}
